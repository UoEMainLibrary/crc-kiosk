@font-face {
    font-family: "press";
    src: url(./fonts/PressStart2P.ttf) format("truetype");
}
@font-face {
    font-family: "zarb";
    src: url(./fonts/zarbville_nbp.ttf) format("truetype");
}
@font-face {
    font-family: "atari";
    src: url(./fonts/atari%20full.ttf) format("truetype");
}
@font-face {
    font-family: "mayfair";
    src: url(./fonts/MAYFAIR_B.ttf) format("truetype");
}
div, h3, h1, h2, h4, p, td, tr, i, b, a {
    font-family: "press";
}

i, b, a, p, td, tr
{
    color: #afd24c;
    font-size:100%;
}
p
{
    text-align: center;
}

h3
{
    color: #afd24c;
    text-align: center;
    font-size:150%;
}

h1, h2
{
    color: #afd24c;
    text-align: center;
    font-size:170%;
}

h4
{
    color: #afd24c;
    font-size:170%;
    text-align: center;
}

td
{
    text-align: center;
}

 input[type="submit"] {
     box-shadow: 0px 0px 9px rgba(30,30,60, 0.3); -moz-box-shadow: 0px 0px 9px rgba(30,30,60, 0.75); -webkit-box-shadow: 0px 0px 9px rgba(30,30,60, 0.3);
     font-size: 16px; color: #afd24c; font-weight: bold;  width: 160px; height: 50px; font-family: "press";
    background: url(./images/Save.jpg) no-repeat 0px 0px;
}
.central
{
    background-color : #333928;
}

.image
{
    margin: auto;
